<!-- Waves Effect Css -->

<section class="content">
    <div class="container-fluid">

        <!-- Exportable Table -->
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <hr>

                <div class="card">
                    <div class="header">
                        <h2>
                            All Users
                        </h2>
                    </div>
                    <div class="body">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped table-hover dataTable js-exportable">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th>Username</th>
                                        <th>Email</th>
                                        <th>Phone</th>
                                        <th>Date of reg.</th>

                                    </tr>
                                </thead>

                                <tbody>
                                    {{#each users}}
                                    <tr>
                                       
                                        <td>{{this.id}}</td>
                                        <td>{{this.username}}</td>
                                        <td>{{this.email}}</td>
                                        <td>{{this.phone}}</td>
                                         <td>{{this.date}}</td>


                                    </tr>
                                    {{/each}}
                                </tbody>

                            </table>
                        </div>


                    </div>

                </div>


            </div>
        </div>
        <!-- #END# Exportable Table -->
    </div>
</section>

<!-- Jquery DataTable Plugin Js -->