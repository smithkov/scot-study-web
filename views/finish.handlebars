</section>
<!-- Jquery DataTable Plugin Js -->
<script>

  function toPdf() {


    
    var doc = new jsPDF()
    doc.setFontSize(26)
    doc.text(20, 20, 'APPLICATION FORM');
    var dob = "{{app.dob}}";
    var programYear = new Date("{{app.hProgrammeYear}}").getFullYear();

    var date = new Date(dob);
    var fullname = '{{app.firstname}} {{app.middlename}}  {{app.surname}}';

    doc.setFontSize(13)
    doc.text(20, 50, 'FULL NAME');

    doc.setFontSize(12);
    doc.text(20, 60,fullname );


    doc.setFontSize(13)
    doc.text(20, 70, 'DATE OF BIRTH');

    doc.setFontSize(12);
    doc.text(20, 80, date.getDay().toString() + "/" + date.getMonth().toString() + "/" + date.getFullYear().toString());

    doc.setFontSize(13)
    doc.text(20, 90, 'NATIONALITY');

    doc.setFontSize(12);
    doc.text(20, 100, '{{app.Country.name}}');

    doc.setFontSize(13)
    doc.text(20, 110, 'GENDER');

    doc.setFontSize(12);
    doc.text(20, 120, '{{app.gender}}');

    doc.setFontSize(13)
    doc.text(20, 130, 'MARITAL STATUS');

    doc.setFontSize(12);
    doc.text(20, 140, '{{app.marital}}');

    doc.setFontSize(13)
    doc.text(20, 150, 'HOME ADDRESS');

    doc.setFontSize(12);
    doc.text(20, 160, '{{app.homeAddress}}');
 
    doc.setFontSize(13)
    doc.text(20, 170, 'POSTAL ADDRESS');

    doc.setFontSize(12);
    doc.text(20, 180, '{{app.postalAddress}}'?'{{app.postalAddress}}':"No Postal Address");

    doc.setFontSize(13)
    doc.text(20, 190, 'PHONE');

    doc.setFontSize(12);
    doc.text(20, 200, '{{app.phone}}     Email:{{app.contactEmail}}');

    doc.setFontSize(13)
    doc.text(20, 210, 'HIGHEST QUALIFICATION');

    doc.setFontSize(12);
    doc.text(20, 220, '{{app.hQualification}}');

    doc.setFontSize(13)
    doc.text(20, 230, 'GRADE');

    doc.setFontSize(12);
    doc.text(20, 240, '{{app.hGrade}}');

    doc.setFontSize(13)
    doc.text(20, 250, 'SCHOOL NAME');

    doc.setFontSize(12);
    doc.text(20, 260, '{{app.hSchoolName}}');

    doc.setFontSize(13)
    doc.text(20, 270, 'COMPLETED? ' + '{{app.hCompleted}}' ? "YES" : "NO");


    doc.setFontSize(13)
    doc.text(20, 280, 'PROGRAMME YEAR');

    doc.setFontSize(12);
    doc.text(20, 290, programYear.toString());

    doc.addPage();

    doc.setFontSize(13)
    doc.text(20, 30, 'ENGLISH TEST');

    doc.setFontSize(12);
    doc.text(20, 40, '{{app.englishTest}}');

    doc.setFontSize(13)
    doc.text(20, 50, 'FIRST COURSE OF INTEREST');

    doc.setFontSize(12);
    doc.text(20, 60, '{{app.course1}}' + " at " + '{{app.schoolWish1}}');

    doc.setFontSize(13)
    doc.text(20, 70, 'SECOND COURSE OF INTEREST');

    doc.setFontSize(12);
    doc.text(20, 80, '{{app.course1}}' + " at " + '{{app.schoolWish2}}');

    doc.setFontSize(13)
    doc.text(20, 90, 'SPONSOR');

    doc.setFontSize(12);
    doc.text(20, 100, '{{app.sponsor}}');

    doc.setFontSize(13)
    doc.text(20, 110, "SPONSOR'S NAME");

    doc.setFontSize(12);
    doc.text(20, 120, '{{app.sponsorName}}');

    doc.setFontSize(13)
    doc.text(20, 130, "SPONSOR'S OCCUPATION");

    doc.setFontSize(12);
    doc.text(20, 140, '{{app.sponsorOccupation}}');

    doc.setFontSize(13)
    doc.text(20, 150, "BUDGET FOR TUITION FEE");

    doc.setFontSize(12);
    doc.text(20, 160, '{{app.budget}}');

    doc.setFontSize(13)
    doc.text(20, 170, "HAS APPLIED FOR VISE BEFORE");

    doc.setFontSize(12);
    doc.text(20, 180, '{{app.hasApplied}}');

    doc.setFontSize(13)
    doc.text(20, 190, "PURPOSE OF TRAVEL");

    doc.setFontSize(12);
    doc.text(20, 200, '{{app.purpose}}');

    var reasonForRefusal = "{{app.reasonOfRefusal}}";
    if (reasonForRefusal != "") {
      doc.setFontSize(13)
      doc.text(20, 210, "REASON OF REFUSAL");

      doc.setFontSize(12);
      doc.text(20, 220, '{{app.reasonOfRefusal}}');
    }



    doc.save(fullname+ new Date() +'.pdf')
  }
  function toggleEditMode(id) {
    if (id == "isUpload") {
      document.getElementById("upload").style.display = "block";
    }
    else {
      document.getElementById(id).disabled = false;
    }
    return false;
  }
  $(document).ready(function () {
    var btnPdf = $("#btnPdf");
    btnPdf.click(function () {
      toPdf();
    })
    $("#upload").hide();

  })

  $(function () {
    var txtReason = $("#txtReason");
    txtReason.hide();
    $("#cboDecision").change(function () {
      if ($(this).val() === "Application Rejected")
        txtReason.show();
      else {
        txtReason.hide();
      }
    })

  });
</script>