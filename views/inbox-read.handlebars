<section class="content">
  <div class="container-fluid">

    <!-- Exportable Table -->
    <div class="row clearfix">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <hr>

        <div class="card">
          <div class="header">
            <h2>
              Sent by {{senderName}}
            </h2>

          </div>
          <div class="body">
            {{message.message}}
          </div>

        </div>


      </div>
    </div>
    <!-- #END# Exportable Table -->
  </div>
</section>


